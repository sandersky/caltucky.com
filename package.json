{
  "name": "caltucky",
  "version": "0.0.0",
  "description": "Frontend for caltucky.com",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "build-dev": "webpack-dev-server",
    "build-server": "babel -o ./server/wordpress.js ./app/actions/wordpress.js && sed -i '' '1s/^/var fetch = require(\"node-fetch\");/' ./server/wordpress.js",
    "deploy": "npm run build && gh-pages -d public -f wordpress.js",
    "server": "npm run build-server && node ./server/index.js",
    "test": "flow && eslint . && jest"
  },
  "author": "Matthew Dahl",
  "license": "MIT",
  "dependencies": {
    "prop-types": "15.5.10",
    "react": "16.0.0-beta.5",
    "react-dom": "16.0.0-beta.5",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "3.2.0",
    "babel-cli": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.5",
    "eslint": "4.5.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.3.0",
    "eslint-plugin-standard": "3.0.1",
    "express": "4.15.4",
    "express-http-proxy": "1.0.6",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "flow-bin": "0.53.1",
    "gh-pages": "1.0.0",
    "jest-cli": "20.0.4",
    "node-fetch": "1.7.2",
    "node-sass": "4.5.3",
    "sass-loader": "6.0.6",
    "uglifyjs-webpack-plugin": "0.4.6",
    "vm2": "^3.5.0",
    "webpack": "3.5.5",
    "webpack-dev-server": "2.7.1"
  }
}
