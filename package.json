{
  "name": "caltucky",
  "version": "0.0.0",
  "description": "Frontend for caltucky.com",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "build-dev": "webpack-dev-server",
    "build-server": "babel -o ./server/wordpress.js ./app/actions/wordpress.js && sed -i '' '1s/^/var fetch = require(\"node-fetch\");/' ./server/wordpress.js",
    "deploy": "npm run build && gh-pages -d public -f wordpress.js",
    "server": "npm run build-server && node ./server/index.js",
    "test": "flow && lintly app/ scripts/ server/ *.js && jest"
  },
  "author": "Matthew Dahl",
  "license": "MIT",
  "dependencies": {
    "nullthrows": "1.0.0",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.36",
    "@babel/core": "7.0.0-beta.36",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.36",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.36",
    "@babel/preset-env": "7.0.0-beta.36",
    "@babel/preset-flow": "7.0.0-beta.36",
    "@babel/preset-react": "7.0.0-beta.36",
    "autoprefixer-loader": "3.2.0",
    "babel-core": "^7.0.0-0",
    "babel-jest": "22.0.4",
    "babel-loader": "7.1.2",
    "babel-plugin-auto-css-modules": "0.0.5",
    "css-loader": "0.28.8",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.0",
    "express": "4.16.2",
    "express-http-proxy": "1.1.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "flow-bin": "0.63.1",
    "gh-pages": "1.1.0",
    "jest-cli": "22.0.4",
    "jsdom": "11.5.1",
    "lintly": "0.0.18",
    "node-fetch": "1.7.3",
    "postcss": "6.0.16",
    "postcss-cssnext": "3.0.2",
    "postcss-for": "2.1.1",
    "postcss-import": "11.0.0",
    "postcss-loader": "2.0.10",
    "uglifyjs-webpack-plugin": "1.1.6",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.10.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "json-summary",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/__mocks__/style.js"
    },
    "setupFiles": [
      "<rootDir>/jest-setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testRegex": "/__tests__/.*-test\\.js$"
  }
}
